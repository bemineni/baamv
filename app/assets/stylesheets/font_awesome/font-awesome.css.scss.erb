@import "variables";
@import "mixins";
//@import "path";
@font-face {
  font-family: 'FontAwesome';
  src: url('<%=asset_path("fontawesome-webfont.eot")%>?v=#{$fa-version}');
  src: url('<%=asset_path("fontawesome-webfont.eot")%>?#iefix&v=#{$fa-version}') format('embedded-opentype'),
    url('<%=asset_path("fontawesome-webfont.woff")%>?v=#{$fa-version}') format('woff'),
    url('<%=asset_path("fontawesome-webfont.ttf")%>?v=#{$fa-version}') format('truetype'),
    url('<%=asset_path("fontawesome-webfont.svg")%>?v=#{$fa-version}#fontawesomeregular') format('svg');
  //src: url('#{$fa-font-path}/FontAwesome.otf') format('opentype'); // used when developing fonts
  font-weight: normal;
  font-style: normal;
}
@import "core";
@import "larger";
